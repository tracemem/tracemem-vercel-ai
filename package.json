{
  "name": "@tracemem/vercel-ai",
  "version": "0.1.2",
  "description": "Vercel AI SDK integration for TraceMem",
  "repository": {
    "type": "git",
    "url": "https://github.com/tracemem/tracemem-vercel-ai.git"
  },
  "homepage": "https://tracemem.com",
  "bugs": {
    "url": "https://github.com/tracemem/tracemem-vercel-ai/issues"
  },
  "author": "reDB <support@redb.co>",
  "license": "Apache-2.0",
  "keywords": [
    "tracemem",
    "vercel",
    "ai",
    "sdk",
    "agents",
    "approvals",
    "governance"
  ],
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/cjs/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js",
      "types": "./dist/cjs/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "rm -rf dist && npm run build:cjs && npm run build:esm && echo '{\"type\": \"module\"}' > dist/esm/package.json && echo '{\"type\": \"commonjs\"}' > dist/cjs/package.json",
    "build:cjs": "tsc -p tsconfig.json",
    "build:esm": "tsc -p tsconfig.esm.json",
    "prepack": "npm run build",
    "test": "jest"
  },
  "peerDependencies": {
    "ai": "^6.0.47"
  },
  "dependencies": {
    "@tracemem/ts-sdk": "^0.1.1",
    "zod": "^3.23.0"
  },
  "devDependencies": {
    "@ai-sdk/provider": "^1.0.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^22.0.0",
    "jest": "^30.2.0",
    "ts-jest": "^29.4.6",
    "ts-node": "^10.9.0",
    "typescript": "^5.0.0"
  }
}